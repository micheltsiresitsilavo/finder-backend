{
  "name": "prisma-express",
  "version": "1.0.0",
  "description": "express server with prisma ORM",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "p:schema": "npx prisma init",
    "p:migrate": "npx prisma migrate",
    "p:schema --sqlite": "npx prisma init --datasource-provider sqlite",
    "start": "nodemon server.js"
  },
  "keywords": [
    "express",
    "prisma",
    "backend",
    "orm"
  ],
  "author": "Michel Tsilavo",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@types/express-formidable": "^1.2.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cloudinary": "^1.35.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-formidable": "^1.2.0",
    "express-validator": "^6.15.0",
    "formidable": "^2.1.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "sanitize-html": "^2.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.3",
    "nodemon": "^2.0.21",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
